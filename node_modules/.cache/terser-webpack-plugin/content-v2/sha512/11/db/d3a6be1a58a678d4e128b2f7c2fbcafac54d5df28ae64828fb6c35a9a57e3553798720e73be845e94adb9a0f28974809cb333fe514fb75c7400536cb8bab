')\n  let [empty, setEmpty] = useState(false)\n  const [filteredResults, setFilteredResults] = useState(Lists);\n\n  useEffect(() => {\n    const filterResults = Lists.filter((item) =>\n      item.title.toLowerCase().includes(search.toLowerCase())\n    );\n    setFilteredResults(filterResults);\n    if (filterResults.length === 0) {\n      setEmpty(true)\n    } else {\n      setEmpty(false)\n    }\n  }, [search]);\n\n  return (\n    <BrowserRouter>\n      <div>\n        <NavBar search={search} setSearch={setSearch} empty={empty} />\n        <Routes>\n          <Route path=\"/\" element={<Main Lists={filteredResults} setSearch={setSearch} search={search} empty={empty} />} />\n          <Route path=\"/product\" element={<Product />} />\n        </Routes>\n      </div>\n    </BrowserRouter>\n  )\n}\n\nexport default App\n","import React from 'react';\nimport ReactDOM from 'react-dom/client';\nimport './index.css';\nimport App from './App';\n\nconst root = ReactDOM.createRoot(document.getElementById('root'));\nroot.render(\n  <React.StrictMode>\n    <App />\n  </React.StrictMode>\n);\n","// extracted by mini-css-extract-plugin\nmodule.exports = {\"wrapper\":\"Items_wrapper__heY14\",\"card\":\"Items_card__2LUFG\",\"list\":\"Items_list__1Y9hx\",\"card2\":\"Items_card2__141Ge\",\"title\":\"Items_title__26OOQ\",\"price\":\"Items_price__HzBEC\",\"button\":\"Items_button__3u3wq\",\"info\":\"Items_info__U1mBv\"};","// extracted by mini-css-extract-plugin\nmodule.exports = {\"cover\":\"Cart_cover__16HnX\",\"navOpen\":\"Cart_navOpen__1IKHD\",\"list2\":\"Cart_list2__2hpcY\",\"close\":\"Cart_close__IYS1s\",\"bar1\":\"Cart_bar1__jnBww\",\"bar2\":\"Cart_bar2__2-QoS\"};","// extracted by mini-css-extract-plugin\nmodule.exports = {\"nav\":\"NavBar_nav__2r9X7\",\"nav_child\":\"NavBar_nav_child__27BV7\",\"nav_first\":\"NavBar_nav_first__3VqfE\",\"nav_search\":\"NavBar_nav_search__3JDum\",\"nav_second\":\"NavBar_nav_second__2orIj\",\"nav_search_mobile\":\"NavBar_nav_search_mobile__3qpOx\